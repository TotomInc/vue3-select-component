{
  "name": "vue3-select-component",
  "type": "module",
  "version": "0.12.3",
  "description": "A flexible & modern select-input control for Vue 3.",
  "author": "Thomas Cazade <cazade.thomas@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TotomInc/vue3-select-component.git"
  },
  "keywords": [
    "vue3",
    "select",
    "dropdown"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:playground": "vite --mode development:playground",
    "test": "vitest --coverage",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --build",
    "bumpp": "bumpp package.json package-lock.json",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "docs:build": "vitepress build docs"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.52.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.0.0",
    "@tsconfig/node22": "22.0.2",
    "@types/jsdom": "27.0.0",
    "@types/node": "24.9.1",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitest/coverage-v8": "4.0.1",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "autoprefixer": "10.4.21",
    "bumpp": "10.3.1",
    "eslint": "9.36.0",
    "jsdom": "27.0.1",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "typescript": "5.9.2",
    "vite": "7.1.12",
    "vite-plugin-css-injected-by-js": "3.5.2",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-vue-devtools": "8.0.3",
    "vitepress": "1.6.4",
    "vitest": "4.0.1",
    "vue": "3.5.22",
    "vue-component-type-helpers": "3.1.1",
    "vue-router": "4.6.3",
    "vue-tsc": "3.1.1"
  }
}
