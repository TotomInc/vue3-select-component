{
  "name": "vue3-select-component",
  "type": "module",
  "version": "0.11.7",
  "description": "A flexible & modern select-input control for Vue 3.",
  "author": "Thomas Cazade <cazade.thomas@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TotomInc/vue3-select-component.git"
  },
  "keywords": [
    "vue3",
    "select",
    "dropdown"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:playground": "vite --mode development:playground",
    "test": "vitest --coverage",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --build",
    "bumpp": "bumpp package.json package-lock.json",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "docs:build": "vitepress build docs"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.41.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.13.2",
    "@tsconfig/node22": "22.0.2",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.15.27",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitejs/plugin-vue-jsx": "4.2.0",
    "@vitest/coverage-v8": "3.1.4",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "autoprefixer": "10.4.21",
    "bumpp": "10.1.1",
    "eslint": "9.22.0",
    "jsdom": "26.1.0",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.4",
    "typescript": "5.8.2",
    "vite": "6.3.5",
    "vite-plugin-css-injected-by-js": "3.5.2",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-vue-devtools": "7.7.6",
    "vitepress": "1.6.3",
    "vitest": "3.1.4",
    "vue": "3.5.16",
    "vue-component-type-helpers": "2.2.10",
    "vue-router": "4.5.1",
    "vue-tsc": "2.2.10"
  }
}
